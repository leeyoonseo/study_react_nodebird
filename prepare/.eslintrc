{
    // 본인의 스타일대로 커스텀하면됨..


    "parserOptions": {
        // 최신문법
        "ecmaVersion": 2020,


        // 모듈시스템(import, export)
        "sourceType": "module",


        // react의 문법
        "ecmaFeatures" : {
            "jsx": true
        },


        // 문법
        "env": {
            "browser": true,
            "node": true,
            "es6": true
        },


        // 실제 규칙, 아래 2개는 너그러운 문법
        // 나중에 엄격한 문법으로 변경해주는게 좋다.
        "extends": [
            "eslint:recommend",
            "plugin:react/recomended"
        ],


        // 플러그인
        "plugins": [
            "import",
            "react-hooks"
        ],


        "rules": {


        }


    }
}